{
  "name": "jsdoc-markdown-cli",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/ErBlack/jsdoc-markdown-cli.git"
  },
  "description": "Cli tool to generate markdown readme.md from jsdocs of module",
  "main": "./bin/index.js",
  "bin": {
    "jsdoc-markdown-cli": "./bin/index.js"
  },
  "keywords": [
    "jsdoc",
    "markdown"
  ],
  "files": [
    "bin",
    "lib"
  ],
  "author": "ertema@yandex-team.ru",
  "license": "MPL-2.0",
  "dependencies": {
    "commander": "^2.19.0",
    "glob": "^7.1.3",
    "jsdoc-to-markdown": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "node": true
    }
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix",
        "git add"
      ]
    }
  }
}
