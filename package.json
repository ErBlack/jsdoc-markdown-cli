{
  "name": "jsdoc-markdown-cli",
  "version": "3.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/ErBlack/jsdoc-markdown-cli.git"
  },
  "description": "Cli tool to generate markdown readme.md from jsdocs of module",
  "main": "./bin/jsdoc-markdown-cli.js",
  "bin": {
    "jsdoc-markdown-cli": "./bin/jsdoc-markdown-cli.js"
  },
  "keywords": [
    "jsdoc",
    "markdown"
  ],
  "files": [
    "bin",
    "lib"
  ],
  "author": "dev@erblack.me",
  "license": "MPL-2.0",
  "dependencies": {
    "commander": "^14.0.3",
    "glob": "^13.0.6",
    "jsdoc-to-markdown": "^9.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.2",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0"
  },
  "scripts": {
    "prepare": "husky",
    "postversion": "git push && git push --tags"
  },
  "nano-staged": {
    "*.js": [
      "eslint --fix"
    ]
  }
}
